{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/checkout",
      "dest": "/checkout.html"
    },
    {
      "src": "/orders",
      "dest": "/orders.html"
    },
    {
      "src": "/tracking",
      "dest": "/tracking.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}